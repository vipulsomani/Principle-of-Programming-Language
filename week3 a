#include<iostream>
using namespace std;
int main()
{
	int a,b;
	char c;
	cout<<"Enter two no for operation";
	cin>>a>>b;
	cout<<"enter operand";
	cin>>c;
	switch(c)
	{
		 
    
        case '+':
            cout <<a<< " + " <<b<< " = " << a+b;
            break;
        case '-':
            cout <<a<< " - " <<b<< " = " << a-b;
            break;
        case '*':
            cout <<a<< " * " <<b<< " = " << a*b;
            break;
        case '/':
            cout <<a<< " / " <<b<< " = " << a/b;
            break;
        case '%':
			cout <<a<< " % " <<b<< " = " <<a%b;
			break; 
        default:
          
            cout << "Error! operator is not correct";
            break;
    
    
	}
	
}
